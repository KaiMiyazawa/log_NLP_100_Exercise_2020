   ],
   "source": [
    "N = int(input())\n",
    "with open(\"popular-names.txt\") as f:\n",
    "    lines = f.readlines()\n",
    "    c = len(lines) - 1\n",
    "    for i in reversed(range(N)):\n",
    "        print(lines[c-i].replace('\\n', ''))"
   ]
